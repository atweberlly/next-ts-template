{
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "pluginSearchDirs": false,
  "plugins": [
    "@prettier/plugin-xml",
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "^react",
    "^next(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^~(.*)$",
    "<TYPES>",
    "<TYPES>^[.]",
    "^[.]"
  ]
}
